package com.spring.common.file;

import java.io.File;

public class FileuploadUtil {
	/* FileUploadUtil 클래스의 모든 메서드는 정적 메서드로 정의한다. 접근방법: 클래스명.메서드명() */
	//파일을 업로드할 폴더 생성.
	public static void makeDir(String docRoot) {
		File fileDir = new File(docRoot);
		if(fileDir.exists()) {
			return;
		}
		fileDir.mkdirs();
	}

}
