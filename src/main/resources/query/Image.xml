<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.msix.client.product.dao.CImageDAO">

	<select id="imageList" parameterType="goods" resultType="clientImage">
   		select i_no, p_no, i_name, i_thumb, i_date from image where p_no = #{p_no}
    </select>

</mapper>